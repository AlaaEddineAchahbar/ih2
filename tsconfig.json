{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "noImplicitReturns": true,
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": false,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "suppressImplicitAnyIndexErrors": true,
    "noImplicitAny": false,
    "importHelpers": true,
    "target": "es2020",
    "allowJs": true,
    "module": "es2020",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2020",
      "dom"
    ]
  },
  "angularCompilerOptions": {
    "strictInjectionParameters": false,
    "strictTemplates": false,
    "fullTemplateTypeCheck": false
  },
  "exclude": [
    "cypress",
    "./cypress.config.ts"
  ]
}